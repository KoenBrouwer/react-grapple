{
  "compilerOptions": {
	"target": "es5",
	"module": "commonjs",
	"declaration": true,
	"outDir": "./dist",
	"strict": true,

	"sourceMap": true,
	"removeComments": true,
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,

	"noImplicitAny": false,
	"jsx": "react",
	"lib": ["dom", "es2015"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
